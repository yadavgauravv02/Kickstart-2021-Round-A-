void solve () {
    int n=0,m=0,a=0,b=0,c=0,d=0,x=0,y=0,z=0,k=0;
    cin>>n>>k;
    string s;
    cin>>s;
    rep(i,0,n/2) {
    	if(s[i]!=s[n-i-1]) x++;
    }
    cout<<abs(k-x)<<en;
}

